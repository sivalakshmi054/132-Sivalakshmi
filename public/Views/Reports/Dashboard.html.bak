<div class="container">
    <div class="row">
        <div class="col-md-12">
            <div class="mainbody container-fluid">
                <p class="page-title">Dashboard</p>
                <div class="row">
                    <div class="col-md-12 col-lg-12">
                        <div class="lineTab">
                            <a href="javascript:" id="A1" class="lineActive linetags"> Attendance</a>
                            <a href="javascript:" id="A9" class="linetags" ng-click="Examcenterwiseattendance()">Exam Center Wise Attendance</a>
                            <a href="javascript:" id="A9" class="linetags" ng-click="Evaluation()">Evaluation</a>
                            <!-- <a href="javascript:" id="A9" class="linetags" ng-click="GradeRedirect()">Mark Analysis</a> -->
                        </div>
                    </div>
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <table st-table="emptydata" style="width: 100%" st-safe-src="AcademicList" class="table table-striped table-bordered">
                            <thead>
                                <tr style="text-align: center">
                                    <th st-ratio="5">#</th>
                                    <th st-ratio="9">Date</th>
                                    <th st-ratio="9">Day</th>
                                    <th st-ratio="20">Exam Name</th>
                                    <th st-ratio="20">Year</th>
                                    <th st-ratio="20">Subject</th>
                                    <th st-ratio="20">Total HT Issued</th>
                                    <th st-ratio="20">Present</th>
                                    <th st-ratio="20">P %</th>
                                    <th st-ratio="20">Absent</th>
                                    <th st-ratio="20">A %</th>
                                </tr>
                            </thead>
                            <tbody>

                                <tr ng-repeat="Subject in AttendanceList">
                                    <td>{{$index+1}}</td>
                                    <td>{{Subject.attendancedate | date : "dd-MM-y"}}</td>
                                    <!-- <td>{{Subject.AttendanceDate | date : 'dd'}}</td> -->
                                    <td>{{Subject.attendanceDay}}</td>
                                    <td>{{Subject.course}}</td>
                                    <td>{{Subject.examname}}</td>
                                    <td>{{Subject.subjectname}}</td>
                                    <td>{{Subject.totalstudent}}</td>
                                    <td>{{Subject.student}}</td>
                                    <td>{{(Subject.student/subject.totalstudent)*100 | number : 2}}%</td>
                                    <td>{{(Subject.totalstudent-subject.student)}}</td>
                                    <td>{{((Subject.totalstudent-subject.student)/Subject.totalstudent)*100 | number : 2}}%</td>

                                </tr>
                            </tbody>

                            <tfoot ng-show="AttendanceList.length >10">
                                <tr>
                                    <td colspan="10" class="text-center">
                                        <items-page items-On-Page="itemsOnPage" total-Items="AttendanceList.length"></items-page>
                                        items per page
                                        <div st-items-by-page="itemsOnPage" st-page-change="rembemberCurrentPage(newPage)"
                                            st-pagination="" st-template="Views/Administration/pagination.custom.html"></div>

                                    </td>
                                </tr>
                            </tfoot>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>